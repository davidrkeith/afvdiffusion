{UTF-8}
Vehicle Sales Percent i[Technology]=
	Vehicle Sales Share i[Technology]*100
	~	dmnl
	~		~	:SUPPLEMENTARY 
	|

Average FE of New Vehicles=
	SUM(FE of New Vehicles by Platform j[Technology!]*Vehicle Sales Share i[Technology!]\
		)
	~	miles/GGE
	~		~	:SUPPLEMENTARY 
	|

Installed Base Percent i[Technology]=
	Installed Base Share i[Technology]*100
	~	dmnl
	~		~	:SUPPLEMENTARY 
	|

Installed Base Share i[Technology]=
	Installed Base i[Technology]/SUM(Installed Base i[Technology!])
	~	dmnl
	~		|

Average Fleet FE=
	SUM(Installed Base Share i[Technology!]*Average FE by Platform i[Technology!])
	~	miles/GGE
	~		~	:SUPPLEMENTARY 
	|

Unit BATTERY Cost i[Technology]=
	IF THEN ELSE
	  (SW Learning=0, 
	    Initial BATTERY Cost i[Technology], 
	    Initial BATTERY Cost i[Technology]*Effect of Experience on Unit BATTERY Cost i[Technology\
		]    
	  )
	~	$/GGE
	~	Cost of the BATTERY per GGE, by platform
	|

ELECTRIC Architecture Cost i[Technology]=
	IF THEN ELSE(SW Learning=0, Initial ELECTRIC Architecture Cost i[Technology], Initial ELECTRIC Architecture Cost i\
		[
	Technology]*
	
	Effect of Experience on ELECTRIC Architecture Cost i[Technology])
	~	$/vehicles
	~	Cost of the ELECTRIC vehicle architecture, by platform
	|

IC Engine Cost i[Technology]=
	IF THEN ELSE(SW Learning=0, Initial IC Engine Cost i[Technology], Initial IC Engine Cost i\
		[Technology]*
	
	Effect of Experience on IC Engine Cost i[Technology])
	~	$/vehicles
	~	Cost of the internal cobustion engine in a vehicle, by platform
	|

Base Vehicle Cost i[Technology]=
	IF THEN ELSE(SW Learning=0, Initial Base Vehicle Cost i[Technology], Initial Base Vehicle Cost i\
		[Technology]*
	
	Effect of Experience on Base Vehicle Cost i[Technology])
	~	$/vehicles
	~	Base cost of a vehicle, by platform
	|

Total Installed Base=
	SUM(Installed Base i[Technology!])
	~	vehicle
	~		|

Average Familiarity i[TechnologyTo]=
	MIN(1, SUM(Average Familiarity ij[Technology!,TechnologyTo]*Installed Base i[Technology\
		!]/Total Installed Base))
	~	dmnl
	~		~	:SUPPLEMENTARY 
	|

Vehicle Sales Share i[Technology]=
	Vehicle Sales i[Technology]/SUM(Vehicle Sales i[Technology!])
	~	dmnl
	~		|

Clustering factor=
	1
	~	dmnl
	~		|

Density of Stations f[Infrastructure]=
	ZIDZ(Available Infrastructure f[Infrastructure],US Area Square Miles*Clustering factor\
		)
	~	stations/(miles*miles)
	~	Average density of refueling stations, by infrastructure
	|

Normalised Station profit Margin f[Infrastructure]=
	ZIDZ(Station Profit f[Infrastructure],Infrastructure Fixed Costs Amortized over 20 years f\
		[Infrastructure])
	~	dmnl
	~	Margin of a refueling station
	|

Combined effect f[Infrastructure]=
	TF Effect of Profit on Desired Infrastructure(Ratio of Recent Profit Margin to Target Profitability f\
		[Infrastructure])*Sensitivity Effect Alpha+Ratio of Projected Utilization and Target Utilization f\
		[Infrastructure]*(1-Sensitivity Effect Alpha)
	~	dmnl
	~		|

Infrastructure Utilization i[CONVENTIONAL]=
	Recent Utilization f[GASPUMP] ~~|
Infrastructure Utilization i[HYBRID]=
	Recent Utilization f[GASPUMP] ~~|
Infrastructure Utilization i[ELECTRIC]=
	Recent Utilization f[PLUG]
	~	dmnl
	~	Perceived utilization of refueling stations for drivers of platform i
	|

Relative Buffer i[Technology]=
	Fuel Buffer i[Technology]/a i[Technology]
	~	dmnl
	~		~	:SUPPLEMENTARY 
	|

BATTERY Capacity=
	ELECTRIC Range*ELECTRIC Energy Efficiency
	~	GGE/vehicle
	~		|

TF Effect of Profit on Desired Infrastructure(
	[(0,0)-(2,2)],(0,0),(1,1),(2,2))
	~	dmnl
	~		|

Direct Exposure to Platform j[TechnologyTo]=
	Exposure from Drivers j[TechnologyTo]
	~	dmnl/year
	~	Effective strength of a contact with both drivers of the platform j and \
		and non-drivers of the platform i to promote the platform j. Measures the \
		effective rate of reducing the gap to full familiarity of platform i with \
		platform j per year.
	|

Total Social Exposure to Platform ij[Technology,TechnologyTo]=
	Total Marketing Exposure j[TechnologyTo]+Cross Platform Exposure ij[Technology,TechnologyTo\
		]+Direct Exposure to Platform j[TechnologyTo]
	~	dmnl/year
	~	Total effective rate of reducing the gap to full familiarity of platform i \
		with platform j per year.
	|

Cross Platform Exposure ij[Technology,TechnologyTo]=
	IF THEN ELSE(Time<Platform Introduction Date j[TechnologyTo], 0, IF THEN ELSE(SW Familiarity Spillovers\
		=0 , 0, SUM(Direct Exposure to Platform j[TechnologyTo]*Familiarity Spillover Matrix jk
	[TechnologyTo,TechnologySpill!])))
	~	dmnl/year
	~	Total strength of a contact with both drivers of the platform j and and \
		non-drivers of the platform i to promote the platform j including platform \
		i spillover. Measures the effective rate of reducing the gap to full \
		familiarity of platform i with platform j per year.
	|

marketing spending START[CONVENTIONAL]=
	0 ~~|
marketing spending START[HYBRID]=
	0 ~~|
marketing spending START[ELECTRIC]=
	0
	~	year
	~		|

extra stations[GASPUMP]=
	0 ~~|
extra stations[PLUG]=
	0
	~	stations/year
	~		|

extra stations DURATION[GASPUMP]=
	0 ~~|
extra stations DURATION[PLUG]=
	0
	~	year
	~		|

extra stations START[GASPUMP]=
	0 ~~|
extra stations START[PLUG]=
	0
	~	year
	~		|

stations incentives DURATION[GASPUMP]=
	0 ~~|
stations incentives DURATION[PLUG]=
	0
	~	year
	~		|

Manufacturer Subsidy i[Technology]= GAME (
	Manufacturer Vehicle Subsidy[Technology]*PULSE(Manufacturer Vehicle Subsidy START[Technology\
		], Manufacturer Vehicle Subsidy DURATION[Technology]))
	~	$/vehicles
	~	Manufacturer subsidy per vehicle, by platform.
	|

marketing spending[CONVENTIONAL]=
	0 ~~|
marketing spending[HYBRID]=
	0 ~~|
marketing spending[ELECTRIC]=
	0
	~	million/year
	~		|

Stations per Year f[Infrastructure]= GAME (
	extra stations[Infrastructure]*PULSE(extra stations START[Infrastructure],extra stations DURATION\
		[Infrastructure]))
	~	stations/year
	~	Governmental program for additional number of stations being introduced \
		(bypassing economic considerations of infrastructure evolution)
	|

Manufacturer Vehicle Subsidy[CONVENTIONAL]=
	0 ~~|
Manufacturer Vehicle Subsidy[HYBRID]=
	0 ~~|
Manufacturer Vehicle Subsidy[ELECTRIC]=
	0
	~	$/vehicles
	~		|

Spending by Platform j[Technology]= GAME (
	marketing spending[Technology]*PULSE(marketing spending START[Technology], marketing spending DURATION\
		[Technology]))
	~	million/year
	~	Additoinal marketing spending to promote the platform j
	|

marketing spending DURATION[CONVENTIONAL]=
	0 ~~|
marketing spending DURATION[HYBRID]=
	0 ~~|
marketing spending DURATION[ELECTRIC]=
	0
	~	year
	~		|

Manufacturer Vehicle Subsidy START[CONVENTIONAL]=
	0 ~~|
Manufacturer Vehicle Subsidy START[HYBRID]=
	0 ~~|
Manufacturer Vehicle Subsidy START[ELECTRIC]=
	0
	~	year
	~		|

stations incentives START[GASPUMP]=
	0 ~~|
stations incentives START[PLUG]=
	0
	~	year
	~		|

Infrastructure Incentive f[Infrastructure]=
	stations incentives[Infrastructure]*PULSE(stations incentives START[Infrastructure],\
		 stations incentives DURATION[Infrastructure])
	~	$/(year*stations)
	~	Government subsidy per station per year to support stations that sell fuel \
		r
	|

stations incentives[GASPUMP]=
	0 ~~|
stations incentives[PLUG]=
	0
	~	$/(year*stations)
	~		|

Manufacturer Vehicle Subsidy DURATION[CONVENTIONAL]=
	0 ~~|
Manufacturer Vehicle Subsidy DURATION[HYBRID]=
	0 ~~|
Manufacturer Vehicle Subsidy DURATION[ELECTRIC]=
	0
	~	year
	~		|

Initial Infrastructure Availability f[GASPUMP]= INITIAL(
	120000) ~~|
Initial Infrastructure Availability f[PLUG]= INITIAL(
	0)
	~	stations
	~	Initial number of refueling stations available
	|

Initial Infrastructure in Construction f[Infrastructure]= INITIAL(
	(Initial Infrastructure Availability f[Infrastructure]*(-Infrastructure Life f[Infrastructure\
		] + Combined effect f[Infrastructure]*Infrastructure Life f[Infrastructure] + Time to Install Infrastructure f\
		[Infrastructure]))/Infrastructure Life f[Infrastructure])
	~	stations
	~	Initial stock of refueling stations being built
	|

Target Profitability f[GASPUMP]= INITIAL(
	Recent Station Profit Margin f[GASPUMP]) ~~|
Target Profitability f[PLUG]=
	0.07
	~	dmnl
	~	Target profitability of refueling stations. Assume it is equal to current \
		profitability of conventional infrastructure initialized in equilibrium
	|

SW Improving FE=
	0
	~	dmnl [0,1,1]
	~		|

Target Utilization f[GASPUMP]= INITIAL(
	Recent Utilization f[GASPUMP]) ~~|
Target Utilization f[PLUG]=
	0.5
	~	dmnl
	~	Target utilization of a refueling station. Assume it is equal to current \
		utilization of conventional infrastructure initialized in equilibrium
	|

Fuel Efficiency i[Technology]=
	Reference Platform Fuel Efficiency i[Technology]*IF THEN ELSE(SW Improving FE=1, Relative Efficiency i\
		[Technology], 1)
	~	GGE/miles
	~	Effective inverted fuel efficiency of platform i
	|

Distance to Station to Buffer i[CONVENTIONAL]=
	SQRT(Average Distance to Station f[GASPUMP]/Unit conversion for distance to station to buffer\
		)*Buffer miles per Distance to Stations parameter ~~|
Distance to Station to Buffer i[HYBRID]=
	SQRT(Average Distance to Station f[GASPUMP]/Unit conversion for distance to station to buffer\
		)*Buffer miles per Distance to Stations parameter ~~|
Distance to Station to Buffer i[ELECTRIC]=
	SQRT(Average Distance to Station f[PLUG]/Unit conversion for distance to station to buffer\
		)*Buffer miles per Distance to Stations parameter
	~	miles/vehicles
	~	A convex function that describes relation between average distance to \
		refueling station and buffer (amount of miles left on a remaining fuel \
		when drivers begin search for a refueling station) drivers choose. Square \
		root provides a good fit to the observed behavior.
	|

Fuel Dispensing Rate r[GASOLINEFUEL]=
	600 ~~|
Fuel Dispensing Rate r[ELECTRICITY]=
	Initial ELECTRIC Dispensing Rate*Effect of Experience on Recharging Rate
	~	GGE/hour
	~	Rate of filing the storage tank of a vehicle from the pump, by fuel
	|

Initial ELECTRIC Dispensing Rate=
	1.5
	~	GGE/hour
	~		|

"Platform From/To ij"[Technology,TechnologyTo]=
	Vehicle Discards i[Technology]*Share ij[Technology,TechnologyTo]
	~	vehicles/year
	~	Number of vehicles of platform i switching to platform j
	|

Market Growth Rate=
	0
	~	dmnl
	~		|

Platform Demand j[TechnologyTo]=
	SUM((Vehicle Discards i[Technology!]*(1+Market Growth Rate))*Share ij[Technology!,TechnologyTo\
		])
	~	vehicles/year
	~	Total demand of platform j
	|

Vehicle Sales i[Technology]=
	Platform Demand j[Technology]
	~	vehicles/year
	~	Sales of new vehicles, by platform
	|

Ratio of Projected Utilization and Target Utilization f[Infrastructure]=
	MAX(0, ZIDZ(Recent Utilization f[Infrastructure], Target Utilization f[Infrastructure\
		] ))
	~	dmnl
	~	Stations future utilization relative to target utilization
	|

Ratio of Recent Profit Margin to Target Profitability f[Infrastructure]=
	ZIDZ( Recent Station Profit Margin f[Infrastructure] , Target Profitability f[Infrastructure\
		] )
	~	dmnl
	~	Stations future profitability relative to target utilization
	|

Desired Infrastructure f[Infrastructure]=
	Available Infrastructure f[Infrastructure]*Combined effect f[Infrastructure]
	~	stations
	~	Desired number of refueling stations
	|

Sensitivity Effect Alpha=
	0.7
	~	dmnl [0,1]
	~		|

Subsidy Phase In and Out time=
	1
	~	years
	~		|

Effect of Experience on Recharging Rate=
	MAX(1, (Cumulative Order Fulfillment j[ELECTRIC]/Initial Expereince with Recharging)\
		^LOG(1+Learning Rate Recharging Rate, 2))
	~	dmnl
	~	Effect of experience on BATTERY cost
	|

MSRP j[Technology]=
	(1+Markup)*Vehicle Cost i[Technology] - SMOOTH(Manufacturer Subsidy i[Technology], Subsidy Phase In and Out time\
		)
	~	$/vehicles
	~	Vehicle recommended price by manufacturer, by platform
	|

VMT Public per Year by Platform and by Fuel ir[Technology,Fuels]=
	Fuel Usage Matrix ir[Technology,Fuels]*VMT SW i[Technology]*VMT per Year i[Technology\
		]
	~	miles/vehicles/year
	~	Number of miles traveled per year by an average vehicle of a platform i on \
		fuel r
	|

Learning Rate Recharging Rate=
	0
	~	dmnl [0,1,0.05]
	~	Technology learning curve rate. Indicates fractional decrease in principal \
		variable with every doubling of experience
	|

Initial Expereince with Recharging=
	5e+07
	~	vehicles
	~		|

Vehicle Incentives j[TechnologyTo]=
	Incentive Value j[TechnologyTo]*Platform Introduced j[TechnologyTo]
	~	$/vehicles
	~	Actual government incentive for vehicles after the platform is introduced, \
		by platform. This variable reduces the effective price customers pay for \
		the vehicle., by platform
	|

EXP Utility j[TechnologyTo]=
	IF THEN ELSE((Platform Introduction Date j[TechnologyTo]>Time), 0,EXP(Utility j[TechnologyTo\
		]*Logit Choice Scaling))
	~	dmnl
	~	Exponent of the utility of a vehicle of the platform j
	|

Cumulative Familiarity ij[Technology,TechnologyTo]= INTEG (
	Familiarity Discards ij[Technology,TechnologyTo]+Familiarity Increase ij[Technology,\
		TechnologyTo]
	
	-Familiarity Forgetting ij[Technology,TechnologyTo]
	
	-Familiarity Sales ij[Technology,TechnologyTo],
		Initial Familiarity ij[Technology,TechnologyTo])
	~	vehicles
	~	Cumulative familiarity measured in drivers of platform i familiar with \
		platform j
	|

Familiarity Increase ij[Technology,TechnologyTo]=
	MAX(0,(1-Average Familiarity ij[Technology,TechnologyTo])*Total Social Exposure to Platform ij\
		[Technology,TechnologyTo]
	*Installed Base i[Technology])
	~	vehicles/year
	~	Increase in familiarity of platform i with platform j per year.
	|

Familiarity Discards ij[Technology,TechnologyTo]=
	SUM(Familiarity swaps[TechnologyFrom!,Technology,TechnologyTo])
	~	vehicles/year
	~	Increase in familiarity of platform i with platform j per year due to sales
	|

Familiarity swaps[TechnologyFrom,Technology,TechnologyTo]=
	"Platform From/To ij"[TechnologyFrom,Technology]*IF THEN ELSE(TechnologyTo=Technology\
		,1,Average Familiarity ij[TechnologyFrom
	,TechnologyTo])
	~	vehicles/year
	~	TechnologyFrom: the previous platform
		Technology: the platform we buy
		TechnologyTo: the platform about which we tracing familiarity
		
		A consumer that switches to a technology will take familiarity of 1.
	|

Effective Price j[TechnologyTo]=
	MSRP j[TechnologyTo]-Vehicle Incentives j[TechnologyTo]
	~	$/vehicles
	~	Retail price of a vehicle, by platform
	|

Platform Range jf[CONVENTIONAL,Infrastructure]=
	IF THEN ELSE(Infrastructure=GASPUMP, "Range by Platform-Fuel ir"[CONVENTIONAL, GASOLINEFUEL\
		], 0) ~~|
Platform Range jf[HYBRID,Infrastructure]=
	IF THEN ELSE(Infrastructure=GASPUMP, "Range by Platform-Fuel ir"[HYBRID, GASOLINEFUEL\
		], 0) ~~|
Platform Range jf[ELECTRIC,Infrastructure]=
	IF THEN ELSE(Infrastructure=PLUG, "Range by Platform-Fuel ir"[ELECTRIC, ELECTRICITY]\
		, 0)
	~	miles/vehicles
	~	Nominal range of a vehicle, by platform and by stations
	|

Vehicle Operating Cost in Cents i[Technology]=
	Vehicle Operating Cost i[Technology]*Cents per Dollar
	~	cents/miles
	~		|

Effective Range ir[CONVENTIONAL,Fuels]=
	IF THEN ELSE(Fuels=GASOLINEFUEL, Effective Range if[CONVENTIONAL,GASPUMP], 0) ~~|
Effective Range ir[HYBRID,Fuels]=
	IF THEN ELSE(Fuels=GASOLINEFUEL, Effective Range if[HYBRID,GASPUMP], 0) ~~|
Effective Range ir[ELECTRIC,Fuels]=
	IF THEN ELSE(Fuels=ELECTRICITY, Effective Range if[ELECTRIC,PLUG],0)
	~	miles/vehicles
	~	Effective useful range of a vehicle accounting for buffer, which is \
		reserved to find a refueling station, by platform and by fuel type
	|

Fuel Search Cost j[TechnologyTo]=
	ZIDZ(Full Refueling Cost i[TechnologyTo]*Cents per Dollar , VMT per Year i[TechnologyTo\
		]*VMT SW i[TechnologyTo])
	~	cents/miles
	~	Total cost associated with fuel search, wait and refuel, by platform
	|

Cents per Dollar=
	100
	~	cents/$
	~	How many cents are in a dollar
	|

Fuel Price r[GASOLINEFUEL]=
	Gasoline Price*Native units to GGE Gasoline ~~|
Fuel Price r[ELECTRICITY]=
	ELECTRICITY Price*Native units to GGE ELECTRICITY
	~	$/GGE
	~	Fuel price without taxes
	|

Energy Storage i[Technology]=
	BATTERY Cost i[Technology]
	~	$/vehicles
	~	Cost of energy storage per vehicle, by platform
	|

Station Profit f[GASPUMP]=
	Pumps per Station f[GASPUMP]*Pump Operating Profits r[GASOLINEFUEL]+
	Ancillary Revenues Per Station[GASPUMP]-
	Infrastructure Fixed Costs Amortized over 20 years f[GASPUMP] + 
	Infrastructure Incentive f[GASPUMP] ~~|
Station Profit f[PLUG]=
	IF THEN ELSE(Platform Introduction Date j[ELECTRIC] > Time, 0 , 
	Pump Operating Profits r[ELECTRICITY]*Pumps per Station f[PLUG]+
	Ancillary Revenues Per Station[PLUG]-
	Infrastructure Fixed Costs Amortized over 20 years f[PLUG]+
	Infrastructure Incentive f[PLUG] )
	~	$/(stations*year)
	~	Yearly profit of a refueling station that sells fuel r
	|

Vehicle range j[TechnologyTo]=
	Platform Range i[TechnologyTo]
	~	miles/vehicles
	~	Actual range of a vehicle without refueling, by platform
	|

Acceleration by Platform j[CONVENTIONAL]=
	GAS Acceleration ~~|
Acceleration by Platform j[HYBRID]=
	HYBRID Acceleration ~~|
Acceleration by Platform j[ELECTRIC]=
	ELECTRIC Acceleration
	~	seconds
	~	Acceleration of a vehicle of platform j
	|

ELECTRIC Share i[Technology]=
	IF THEN ELSE(Affinity ij[Technology,ELECTRIC]=0, 0 , ZIDZ(Affinity ij[Technology,ELECTRIC\
		],Logit Denominator i
	[Technology]) )
	~	dmnl
	~	Share of drivers of platform i choosing ELECTRIC platform
	|

Platform Introduction Date j[CONVENTIONAL]=
	GAS Introduction Date ~~|
Platform Introduction Date j[HYBRID]=
	HYBRID Introduction Date ~~|
Platform Introduction Date j[ELECTRIC]=
	ELECTRIC Introduction Date
	~	year
	~	Year of introduction of the platform j
	|

Tank GGE j[CONVENTIONAL]=
	GAS Tank/Native units to GGE Gasoline ~~|
Tank GGE j[HYBRID]=
	HYBRID Tank/Native units to GGE Gasoline ~~|
Tank GGE j[ELECTRIC]=
	0
	~	GGE/vehicles
	~		|

BATTERY i[CONVENTIONAL]=
	0 ~~|
BATTERY i[HYBRID]=
	HYBRID Storage ~~|
BATTERY i[ELECTRIC]=
	BATTERY Capacity
	~	GGE/vehicles
	~	Capacity of the BATTERY, by platform
	|

Initial Fleet Fuel Economy GGE i[CONVENTIONAL]=
	Initial GAS FE GGE ~~|
Initial Fleet Fuel Economy GGE i[HYBRID]=
	Initial HYBRID FE GGE ~~|
Initial Fleet Fuel Economy GGE i[ELECTRIC]=
	Initial ELECTRIC FE GGE
	~	miles/GGE
	~	Initial fuel efficiency of a vehicle of platform i
	|

Top Speed by Platform j[CONVENTIONAL]=
	GAS Top Speed ~~|
Top Speed by Platform j[HYBRID]=
	HYBRID Top Speed ~~|
Top Speed by Platform j[ELECTRIC]=
	ELECTRIC Top Speed
	~	miles/hour
	~	Top speed of a vehicle of platform j
	|

Effective Range if[CONVENTIONAL, Infrastructure]=
	IF THEN ELSE(Infrastructure=GASPUMP, MAX(Platform Range jf[CONVENTIONAL, GASPUMP]-Fuel Buffer i\
		[CONVENTIONAL],0), 0) ~~|
Effective Range if[HYBRID, Infrastructure]=
	IF THEN ELSE(Infrastructure=GASPUMP, MAX(Platform Range jf[HYBRID, GASPUMP]-Fuel Buffer i\
		[HYBRID],0), 0) ~~|
Effective Range if[ELECTRIC,Infrastructure]=
	IF THEN ELSE(Infrastructure=PLUG, MAX(Platform Range jf[ELECTRIC, PLUG]-Fuel Buffer i\
		[ELECTRIC],0), 0)
	~	miles/vehicles
	~	Effective useful range of a vehicle accounting for buffer, which is \
		reserved to find a refueling station, by platform and by stations
	|

Emissions per Mile j[TechnologyTo]=
	Emissions per Mile Gasoline j[TechnologyTo]+
	Emissions per Mile HYBRID j[TechnologyTo]+
	Emissions per Mile of ELECTRIC j[TechnologyTo]
	~	tonnes CO2e/miles
	~	Emissions per mile of new vehicles, by platform
	|

Vehicle Operating Cost i[CONVENTIONAL]=
	ZIDZ(Retail Fuel Price r[GASOLINEFUEL],FE of New Vehicles by Platform j[CONVENTIONAL\
		]) ~~|
Vehicle Operating Cost i[HYBRID]=
	ZIDZ(Retail Fuel Price r[GASOLINEFUEL], FE of New Vehicles by Platform j[HYBRID] ) ~~|
Vehicle Operating Cost i[ELECTRIC]=
	ZIDZ(Retail Fuel Price r[ELECTRICITY], FE of New Vehicles by Platform j[ELECTRIC] )
	~	$/miles
	~		|

GHG Emissions Factor r[GASOLINEFUEL]=
	GHG Emissions Factor GAS GGE ~~|
GHG Emissions Factor r[ELECTRICITY]=
	GHG Emissions Factor ELECTRIC GGE
	~	tonnes CO2e/GGE
	~	Greenhouse gas emissions, by fuel
		
		http://www.epa.gov/greenpower/pubs/calcmeth.htm
		GREET
	|

Share ij[Technology,CONVENTIONAL]=
	Gasoline Share i[Technology] ~~|
Share ij[Technology,HYBRID]=
	Hybrid Share i[Technology] ~~|
Share ij[Technology,ELECTRIC]=
	ELECTRIC Share i[Technology]
	~	dmnl
	~	Share of drivers of platform i choosing platform j given current level of \
		consumer utility and familiarity
	|

"Cumulative R&D for Fuel Efficiency"= INTEG (
	"R&D for Fuel Efficiency",
		"Initial R&D for Fuel Efficiency")
	~	$
	~	Cumulative R&D spending on fuel efficiency improvement
	|

Vehicle Cost i[Technology]=
	Base Vehicle Cost i[Technology]+IC Engine Cost i[Technology]+ELECTRIC Architecture Cost i\
		[Technology]+Energy Storage i[
	Technology]
	~	$/vehicles
	~		|

Logit Denominator i[Technology]=
	IF THEN ELSE(Affinity ij[Technology,CONVENTIONAL]=0, 0, Affinity ij[Technology,CONVENTIONAL\
		])+
	IF THEN ELSE(Affinity ij[Technology,HYBRID]=0, 0, Affinity ij[Technology,HYBRID])+
	IF THEN ELSE(Affinity ij[Technology,ELECTRIC]=0, 0, Affinity ij[Technology,ELECTRIC]\
		)
	~	dmnl
	~	Total of affinities of drivers of platform i across all possible platforms \
		j
	|

Emissions per Mile i[Technology]=
	Emissions per Mile of HYBRID i[Technology]+
	Emissions per Mile Gasoline i[Technology]+
	Emissions per Mile ELECTRIC i[Technology]
	~	tonnes CO2e/miles
	~	Emissions per mile of all vehicles, by platform
	|

New Buffer i[Technology]=
	MIN(a i[Technology]*Maximum Buffer, Distance to Station to Buffer i[Technology])
	~	miles/vehicles
	~	Buffer bound by maximum buffer size
	|

Maximum Buffer=
	0.99
	~	dmnl
	~	Maximum fraction of a full range of the vehicle that drivers could use as \
		a buffer. It is a complement to 1 of a minimum useful driving range \
		drivers are expecting from a car
	|

"R&D for Fuel Efficiency"=
	SUM("R&D For Fuel Efficiency i"[Technology!])
	~	$/year
	~	Change in R&D spending on fuel efficiency improvement
	|

Fuel Efficiency Beta=
	-LOG(1-Fuel Efficiency Learning Rate, 2)
	~	dmnl
	~	Fuel Efficiency learning curve strength
	|

Incentive Value j[CONVENTIONAL]=
	0 ~~|
Incentive Value j[HYBRID]=
	0 ~~|
Incentive Value j[ELECTRIC]=
	0
	~	$/vehicles
	~	Government incentive for vehicles, by platform. This variable reduces the \
		effective price customers pay for the vehicle.
	|

Service Cost i[Technology]=
	MAX(0, Value of Time*(Transaction Time+(a i[Technology]-New Buffer i[Technology])/(Fuel Dispensing Rate i\
		[Technology]*FE i
	[Technology])) - Offset of Service Cost by other useful activities i[Technology])
	~	$/vehicle
	~	Cost of filling a storage of a vehicle of platform i from pump
	|

Increase in FE i[Technology]=
	FE of New Vehicles by Platform j[Technology]*Order Fulfillment j[Technology]
	~	miles*vehicles/GGE/year
	~	Increase in fuel efficiency of all vehicles of platform i due to new \
		vehicle sales
	|

VMT per Year i[Technology]=
	12000
	~	miles/vehicles/year
	~	Miles traveled per year by an average vehicle, by platform
	|

Effect of Learning on Fuel Efficiency=
	ZIDZ("Cumulative R&D for Fuel Efficiency","Initial R&D for Fuel Efficiency")^-Fuel Efficiency Beta
	~	dmnl
	~	Effect of R&D on fuel efficiency
	|

Retail Fuel Price r[Fuels]=
	(Fuel Price r[Fuels]+Carbon Tax r[Fuels])
	~	$/GGE
	~	Retail price of a fuel, by fuel type
	|

Gasoline Price Rate=
	Initial Price Gasoline*Total Simulation Price Change Rate GASOLINE/FINAL TIME
	~	($/gallon)/year
	~	Change of price of gasoline
	|

Emissions per Mile HYBRID j[TechnologyTo]=
	IF THEN ELSE(TechnologyTo=HYBRID, ZIDZ(GHG Emissions Factor r[GASOLINEFUEL],FE of New Vehicles by Platform j\
		[TechnologyTo]
), 0)
	~	tonnes CO2e/miles
	~	Emissions per mile of new vehicles using hybrid drivetrain
	|

"R&D For Fuel Efficiency i"[Technology]=
	OEM Revenue i[Technology]*Fraction of Industry Revenue for Fuel Efficiency RandD
	~	$/year
	~	R&D budget for fuel efficiency improvement
	|

Average FE by Platform i[Technology]=
	XIDZ(Cumulative FE of Vehicle by Platform i[Technology], Installed Base i[Technology\
		], FE of New Vehicles by Platform j[Technology] )
	~	miles/GGE
	~	Average fuel efficiency of a vehicle of platform i
	|

FE of New Vehicles by Platform j[Technology]=
	IF THEN ELSE(Fuel Efficiency i[Technology]=0,0,1/Fuel Efficiency i[Technology])
	~	miles/GGE
	~	Average fuel efficiency of a new vehicle of platform i
	|

Regular Marketing Spending i[Technology]=
	OEM Revenue i[Technology]/Dollars per Million*Marketing Fraction of Revenue
	~	million/year
	~	Actual marketing spending of OEMs, by platform
	|

Emissions per Mile Gasoline j[TechnologyTo]=
	IF THEN ELSE(TechnologyTo=CONVENTIONAL, ZIDZ(GHG Emissions Factor r[GASOLINEFUEL],FE of New Vehicles by Platform j\
		[TechnologyTo]
), 0)
	~	tonnes CO2e/miles
	~	Emissions per mile of new vehicles using gasoline
	|

Emissions per Mile of ELECTRIC j[TechnologyTo]=
	IF THEN ELSE(TechnologyTo=ELECTRIC, ZIDZ(GHG Emissions Factor r[ELECTRICITY], FE of New Vehicles by Platform j\
		[ELECTRIC]), 0 )
	~	tonnes CO2e/miles
	~	Emissions per mile of new vehicles using the ELECTRICITY
	|

Total Simulation Price Change Rate GASOLINE= GAME (
	0)
	~	dmnl
	~	Total change in price of gasoline by the end of simulation. Assuming \
		linear ramp.
	|

"GHG Emissions Factor - ELECTRICITY"=
	0.023
	~	tonnes CO2e/GGE
	~	Emissions factor of ELECTRICITY
		The US grid has 0.00069 tonnes CO2 / kWh 
		http://www.epa.gov/energy/ghg-equivalencies-calculator-calculations-and-references
		and with 33.4 kWh/GGE, 0.00069 * 33.4 = 0.023 tonnes CO2/GGE.
	|

Initial Price ELECTRICITY=
	5
	~	$/GGE
	~	Price of the ELECTRICITY at the beginning of simulation
	|

Gasoline Price= INTEG (
	Gasoline Price Rate,
		Initial Price Gasoline)
	~	$/gallon
	~	Current price of gasoline
	|

Initial Price Gasoline=
	3.6
	~	$/gallon
	~	Price of gasoline at the beginning of simulation
	|

ELECTRICITY Price= INTEG (
	ELECTRICITY Price Rate,
		Initial Price ELECTRICITY)
	~	$/GGE
	~	Current price of the ELECTRICITY
	|

ELECTRICITY Price Rate=
	Initial Price ELECTRICITY*Total Simulation Price Change Rate ELECTRICITY/FINAL TIME
	~	$/GGE/year
	~	Change of price of the ELECTRICITY
	|

"Cumulative Experience - BATTERY i"[Technology]= INTEG (
	"Experience Gain - BATTERY i"[Technology],
		Initial Experience BATTERY i[Technology])
	~	GGE
	~	Cumulative experience with BATTERY technology
	|

Wait Cost i[Technology]=
	Value of Time*Target Queue Time i[Technology]*
	ZIDZ( ( XIDZ(1 , (1-Infrastructure Utilization i[Technology])^Alpha, 1e+20) - 1 ) , \
		ZIDZ( Target Utilization i[Technology
	] , (1 - Target Utilization i[Technology])) )
	~	$/vehicle
	~	Cost of waiting in queue at a refueling station for a driver of platform i
	|

Order Fulfillment j[TechnologyTo]=
	Platform Demand j[TechnologyTo]
	~	vehicles/year
	~	Number of vehicles manufactured per year, by platform
	|

Learning Share ia[Technology,Attribute]=
	Initial Learning Share ia[Technology,Attribute]
	~	dmnl
	~	Share of learning experience effect on architecture a of platform i
	|

Current Carbon Price=
	0
	~	$/tonnes CO2e [?,?,5]
	~	Current carbon price of tonne CO2e
	|

Probability OOF i[CONVENTIONAL]=
	EXP(-Density of Stations f[GASPUMP]*PI*New Buffer i[CONVENTIONAL]^2*Average Distance Unit Correction\
		) ~~|
Probability OOF i[HYBRID]=
	EXP(-Density of Stations f[GASPUMP]*PI*New Buffer i[HYBRID]^2*Average Distance Unit Correction\
		) ~~|
Probability OOF i[ELECTRIC]=
	EXP(-Density of Stations f[PLUG]*PI*New Buffer i[ELECTRIC]^2*Average Distance Unit Correction\
		)
	~	dmnl
	~	Probability of running out of fuel for platform i
	|

Suggested Buffer i[Technology]=
	IF THEN ELSE(Platform Introduced j[Technology]=1, New Buffer i[Technology], New Buffer i\
		[Technology])
	~	miles/vehicles
	~	The buffer suggested by the model bound and adjusted for the platform \
		introduction
	|

Unit conversion for distance to station to buffer=
	1
	~	miles/vehicles
	~	Dummy variable to correct for unit errors
	|

OOF Cost i[Technology]=
	Value of Time*"Out-of-Fuel Recovery Time"*Probability OOF i[Technology]
	~	$/vehicle
	~	Cost of running out of fuel for a driver of platform i
	|

Buffer miles per Distance to Stations parameter=
	20
	~	miles/vehicles
	~	A parameter scaling distance to stations curve to buffer miles
	|

Offset of Service Cost by other useful activities i[Technology]=
	Time in other useful activities i[Technology]*Value of Time
	~	$/vehicle
	~	Offset of cost of filling the storage of platform i due to some other \
		useful things a driver can do during the refueling (shopping, eating etc.)
	|

Utility j[TechnologyTo]=
	MAX(Min Utility, Platform Introduced j[TechnologyTo]*(U1 j[TechnologyTo]+U2 j[TechnologyTo\
		]+U3 j[TechnologyTo]+U4 j[TechnologyTo]+U5 j[TechnologyTo]+U6 j[TechnologyTo]+U7 j[\
		TechnologyTo]+U8 j[TechnologyTo]))
	~	dmnl
	~	Von Neumann-Morgenstern consumer utility of a vehicle of the platform j
	|

Min Utility=
	-20
	~	dmnl
	~	Min utility bound to prevent floating point overflow from very large \
		negative numbers
	|

Gasoline Share i[Technology]=
	IF THEN ELSE(Affinity ij[Technology,CONVENTIONAL]=0, 0 , ZIDZ(Affinity ij[Technology\
		,CONVENTIONAL],Logit Denominator i[Technology]) )
	~	dmnl
	~	Share of drivers of platform i choosing gasoline platform
	|

Logit Choice Scaling=
	1
	~	dmnl
	~	Scaling factor of MNL choice model
	|

Hybrid Share i[Technology]=
	IF THEN ELSE(Affinity ij[Technology,HYBRID]=0, 0 , ZIDZ(Affinity ij[Technology,HYBRID\
		],Logit Denominator i[Technology]) )
	~	dmnl
	~	Share of drivers of platform i choosing hybrid platform
	|

Fuel Consumed per Platform ir[Technology,Fuels]=
	ZIDZ(VMT Public per Year by Platform and by Fuel ir[Technology,Fuels],FE by Platform by Fuel ir\
		[Technology,Fuels])*Installed Base i[Technology]
	~	GGE/year
	~	Total amount of fuel r consumed by a platform i per year
	|

Recent Station Profit Margin f[Infrastructure]=
	SMOOTH(Normalised Station profit Margin f[Infrastructure],Infrastructure Perception Time\
		)
	~	dmnl
	~		|

Emissions per Mile ELECTRIC i[Technology]=
	IF THEN ELSE(Technology=ELECTRIC, ZIDZ(GHG Emissions Factor r[ELECTRICITY], Average FE by Platform i\
		[ELECTRIC]) , 0 )
	~	tonnes CO2e/miles
	~	Emissions per mile of all vehicles using the ELECTRICITY
	|

U2 j[TechnologyTo]=
	Operating Cost Weight*Vehicle Operating Cost in Cents i[TechnologyTo]
	~	dmnl
	~	Operating cost component of von Neumann-Morgenstern utility function
	|

Emissions per Mile Gasoline i[Technology]=
	IF THEN ELSE(Technology=CONVENTIONAL, ZIDZ(GHG Emissions Factor r[GASOLINEFUEL], Average FE by Platform i\
		[Technology] ) , 0)
	~	tonnes CO2e/miles
	~	Emissions per mile of all vehicles using gasoline
	|

Desired Infrastructure Aquisition Rate f[Infrastructure]=
	Infrastructure Loss Rate f[Infrastructure]+Infrastructure Stock Level Adjustment f[Infrastructure\
		]
	~	stations/year
	~	Desired rate of orders for new stations
	|

Desired Infrastructure Under Construction f[Infrastructure]=
	Time to Install Infrastructure f[Infrastructure] * Desired Infrastructure Aquisition Rate f\
		[Infrastructure]
	~	stations
	~	Desired level of stations being built
	|

Infrastructure Stock Level Adjustment f[Infrastructure]=
	ZIDZ( Desired Infrastructure f[Infrastructure]-Available Infrastructure f[Infrastructure\
		], Time to Install Infrastructure f[Infrastructure] )
	~	stations/year
	~	Adjustment for the rate of orders for new stations from comparing current \
		and desired stock level
	|

Infrastructure Supply Line Adjustment f[Infrastructure]=
	(Desired Infrastructure Under Construction f[Infrastructure]-Infrastructure in Construction f\
		[Infrastructure])/Supply Line Adjustment Time f[Infrastructure]
	~	stations/year
	~	Adjustment for the new orders for refueling stations by accounting for the \
		supply line of stations being built
	|

Full Refueling Cost i[Technology]=
	(Search Cost i[Technology]+Wait Cost i[Technology]+Service Cost i[Technology]+OOF Cost i\
		[Technology])*Number of Refuels per Year i
	[Technology]*Platform Introduced j[Technology]*IF THEN ELSE(Refueling possibility i[\
		Technology]=0, 2, 1)
	~	$/(year*vehicles)
	~	Full cost associated with search, wait, and refueling of a vehicle of \
		platform i
	|

Search Cost i[Technology]=
	Value of Time*Average Distance to Station i[Technology]/Average Speed
	~	$/vehicle
	~	Cost of searching for a refueling station for a driver of platform i
	|

Refueling possibility i[Technology]=
	IF THEN ELSE(Average Distance to Station i[Technology]>=Platform Range i[Technology]\
		, 0, 1)
	~	dmnl
	~	Trigger that is 1 when platform range is enough to reach a refueling \
		station
	|

Range Weight= GAME (
	0.2)
	~	dmnl
	~	Weight of the range in the consumer utility
	|

Effective Base Vehicle Spillover Matrix ij[Technology,TechnologyTo]=
	IF THEN ELSE (Technology=TechnologyTo, 1, Extent of Base Vehicle Technology Learning Spillovers\
		)
	~	dmnl
	~	Effective spillovers of base vehicle architecture from platform i to \
		platform j
	|

Number of Refuels per Year i[Technology]=
	VMT i[Technology]/(a i[Technology]-Fuel Buffer i[Technology])
	~	1/year
	~	Number of refuels an average vehicle of platform i needs per year
	|

Extent of Base Vehicle Technology Learning Spillovers= GAME (
	0.85)
	~	dmnl
	~	Spillover effect of base vehicle technology (on a scale from 0 to 1)
	|

Effective ICE Technology Array ij[Technology,TechnologyTo]=
	IF THEN ELSE (Technology=TechnologyTo, 1, ICE Technology Matrix ij[Technology,TechnologyTo\
		]*Extent of ICE Technology Learning Spillovers
	)
	~	dmnl
	~	Effective spillovers of internal combustion engine technology from \
		platform i to platform j
	|

Max Utility from Range=
	Range A^2/(4*Range B)
	~	dmnl
	~	The maximum utility a consumer gets from increased vehicle range
	|

Extent of ICE Technology Learning Spillovers= GAME (
	0.85)
	~	dmnl
	~	Spillover effect of internal combustion engine technology (on a scale from \
		0 to 1)
	|

ELECTRIC Vehicle Technology Matrix ij[Technology,TechnologyTo]=
	IF THEN ELSE(Technology=HYBRID,
	  IF THEN ELSE(TechnologyTo=ELECTRIC,0.7,0),
	IF THEN ELSE(Technology=ELECTRIC,
	  IF THEN ELSE(TechnologyTo=HYBRID,0.7,0),
	 0))
	~	dmnl
	~	Mapping of possible spillovers of ELECTRIC vehicle technology from \
		platform i to platform j
	|

Range at Peak Utility=
	Range A/(2*Range B)*Vehicle Range Normalizer
	~	miles/vehicles
	~	Range that yields the max utility given the weights
	|

U7 j[TechnologyTo]=
	Fuel Search Cost j[TechnologyTo]*Fuel Search Cost Weight*SW Endogenous Infrastructure
	~	dmnl
	~	Fuel search component of von Neumann-Morgenstern utility function
	|

Fuel Search Cost Weight=
	-0.17
	~	dmnl/(cents/miles)
	~	Weight of the fuel search cost in the consumer utility
	|

Vehicle Range Normalizer=
	100
	~	miles/vehicles
	~	Dummy variable for unit corrections
	|

U5 j[TechnologyTo]=
	IF THEN ELSE(Vehicle range j[TechnologyTo] >= Range at Peak Utility, Max Utility from Range\
		, Range A*(Vehicle range j[TechnologyTo
	]/Vehicle Range Normalizer) - Range B*(Vehicle range j[TechnologyTo]/Vehicle Range Normalizer\
		)^2)*Range Weight
	~	dmnl
	~	Range component of von Neumann-Morgenstern utility function
	|

Time in other useful activities i[Technology]=
	0,0,0.25
	~	hour/vehicle
	~	When fueling, drivers/passengers may be able to carry out other activities \
		that are useful so that the fueling time is not "wasted" and should not \
		count against the utility of the vehicle.  We assume that the time spent \
		in other useful activities is zero for all platforms except ELECTRIC, \
		which have a large refuel time, during which people may be either at work, \
		shopping, or in other useful activities. Since not all occasions for \
		refueling an ELECTRIC will be in such circumstances, we assume a lower \
		value for the saved time.
	|

Range A=
	1.268
	~	dmnl
	~	Brownstone Bunch and Train (http://www.uctc.net/papers/597.pdf) estimate \
		an attribute formulation for range, the upward sloping segment of a \
		negative quadratic: 1.268*Range-0.116*(Range^2)
	|

Range B=
	0.116
	~	dmnl
	~	Brownstone Bunch and Train (http://www.uctc.net/papers/597.pdf) estimate \
		an attribute formulation for range, the upward sloping segment of a \
		negative quadratic: 1.268*Range-0.116*(Range^2)
	|

Initial GAS FE GGE=
	Initial GAS FE*Native units to GGE Gasoline
	~	miles/GGE
	~	Initial fuel efficiency of a vehicle of gasoline platform in GGE
	|

GHG Emissions Factor GAS GGE=
	"GHG Emissions Factor - Gasoline"*Native units to GGE Gasoline
	~	tonnes CO2e/GGE
	~	Emissions of gasoline per GGE consumed
	|

"Vehicle Fuel Capacity by Platform-Fuel ir"[CONVENTIONAL,Fuels]=
	IF THEN ELSE(Fuels=GASOLINEFUEL, Tank GGE j[CONVENTIONAL] , 0 ) ~~|
"Vehicle Fuel Capacity by Platform-Fuel ir"[ELECTRIC,Fuels]=
	IF THEN ELSE(Fuels=ELECTRICITY, BATTERY i[ELECTRIC]/Native units to GGE ELECTRICITY \
		, 0 ) ~~|
"Vehicle Fuel Capacity by Platform-Fuel ir"[HYBRID,Fuels]=
	IF THEN ELSE(Fuels =GASOLINEFUEL, Tank GGE j[HYBRID] , 0 )
	~	GGE/vehicles
	~	Capacity of fuel r in a vehicle of platform i
	|

Initial HYBRID FE GGE=
	Initial HYBRID FE*Native units to GGE Gasoline
	~	miles/GGE
	~	Initial fuel efficiency of a vehicle of hybrid platform in GGE
	|

Buffer Change Rate[Technology]=
	Platform Introduced j[Technology]*(Suggested Buffer i[Technology]-Fuel Buffer i[Technology\
		])/(Buffer Adjust Time)
	~	miles/vehicles/year
	~	Rate of change of fuel buffer. Information delay structure
	|

Reference Platform Fuel Efficiency i[Technology]=
	IF THEN ELSE(Initial Fleet Fuel Economy GGE i[Technology]=0,0,1/Initial Fleet Fuel Economy GGE i\
		[Technology])
	~	GGE/miles
	~	Reference inverted fuel efficiency of platform i
	|

Emissions per Mile of HYBRID i[Technology]=
	IF THEN ELSE(Technology=HYBRID, ZIDZ(GHG Emissions Factor r[GASOLINEFUEL],Average FE by Platform i\
		[Technology]), 0)
	~	tonnes CO2e/miles
	~	Emissions per mile of all vehicles using hybrid drivetrain
	|

Initial ELECTRIC FE GGE=
	Initial ELECTRIC FE
	~	miles/GGE
	~	Initial fuel efficiency of a vehicle of ELECTRIC platform in GGE
	|

GHG Emissions Factor ELECTRIC GGE=
	"GHG Emissions Factor - ELECTRICITY"*Native units to GGE ELECTRICITY
	~	tonnes CO2e/GGE
	~	Emissions of the ELECTRICITY per GGE consumed
	|

Fuel Buffer i[Technology]= INTEG (
	Buffer Change Rate[Technology],
		IF THEN ELSE(Platform Introduced j[Technology]=1, New Buffer i[Technology], New Buffer i\
		[Technology]))
	~	miles/vehicles
	~	Perceived fuel buffer (amount of miles left on a remaining fuel when \
		drivers begin search for a refueling station)
	|

FE by Platform by Fuel ir[Technology,Fuels]=
	Fuel Usage Matrix ir[Technology,Fuels]*Average FE by Platform i[Technology]
	~	miles/GGE
	~	Average fuel economy of a platform i using fuel r
	|

Buffer Adjust Time=
	TIME STEP * 2
	~	year
	~	Time to change the buffer
	|

Infrastructure Order Rate f[Infrastructure]=
	MAX(0, Desired Infrastructure Aquisition Rate f[Infrastructure] + Infrastructure Supply Line Adjustment f\
		[Infrastructure])
	~	stations/year
	~	Order rate of new refueling stations
	|

VMT i[CONVENTIONAL]=
	VMT Public per Year by Platform and by Fuel ir[CONVENTIONAL,GASOLINEFUEL] ~~|
VMT i[HYBRID]=
	VMT Public per Year by Platform and by Fuel ir[HYBRID,GASOLINEFUEL] ~~|
VMT i[ELECTRIC]=
	VMT Public per Year by Platform and by Fuel ir[ELECTRIC,ELECTRICITY]
	~	miles/vehicles/year
	~	Number of miles traveled per year by an average vehicle of a platform i
	|

Fraction of Industry Revenue for Fuel Efficiency RandD= GAME (
	0.02)
	~	dmnl
	~	Fraction of revenue for fuel efficiency improvement
	|

Native units to GGE Gasoline=
	1
	~	gallon/GGE
	~	Conversion rate from native capacity units for gasoline (gallons used in \
		the market) to GGE
	|

Relative Efficiency i[Technology]=
	1+(Fuel Efficiency Spillover Strength i[Technology]*(Effect of Learning on Fuel Efficiency\
		-1))
	~	dmnl
	~	Normlalized fuel efficiency of platform i (from 0 to 1)
	|

Carbon Tax r[Fuels]=
	Current Carbon Price*GHG Emissions Factor r[Fuels]
	~	$/GGE
	~	Carbon tax on fuel based on emission impact
	|

Fuel Efficiency Spillover Strength i[Technology]=
	1,0.8,0.5
	~	dmnl
	~	Strength of spillovers of fuel efficiency technology for platform i
	|

Fuel Efficiency Learning Rate= GAME (
	0.2)
	~	dmnl
	~	Fuel Efficiency learning curve rate. Indicates fractional decrease in fuel \
		efficiency with every doubling of R&D expenses
	|

"Initial R&D for Fuel Efficiency"=
	5e+10
	~	$
	~	Initial R&D spending on fuel efficiency improvement
	|

"Initial Installed Base - Vehicles 0 to 4 years i"[Technology]=
	((Vehicles Retirement Fraction*Aging Time Lambda*(1 + Vehicles 5 to 8 Discard Fraction\
		*Aging Time Lambda)*(1 + Vehicles 9 to 12 Discard Fraction*Aging Time Lambda))/
	  (1 + Vehicles Retirement Fraction*Aging Time Lambda*(3 + Aging Time Lambda*(Vehicles 5 to 8 Discard Fraction\
		 + 2*Vehicles 9 to 12 Discard Fraction + Vehicles 5 to 8 Discard Fraction*Vehicles 9 to 12 Discard Fraction\
		*Aging Time Lambda))))*Light Vehicle Market Size j[Technology]
	~	vehicles
	~	Initial installed base of new vehicles given the total fleet size
	|

Cumulative FE of Vehicle by Platform i[Technology]= INTEG (
	Increase in FE i[Technology]-Decrease in FE i[Technology],
		Installed Base i[Technology]*Initial Fleet Fuel Economy GGE i[Technology])
	~	miles*vehicles/GGE
	~	Cumulative fuel efficiency of a vehicle of platform i
	|

Decrease in FE i[Technology]=
	Vehicle Discards i[Technology]*Average FE by Platform i[Technology]
	~	miles*vehicles/GGE/year
	~	Decrease in fuel efficiency of all vehicles of platform i due to vehicle \
		discards
	|

Average Distance to Station f[Infrastructure]=
	SQRT(XIDZ(1, Density of Stations f[Infrastructure]*Average Distance Unit Correction,\
		 1e+20))/2
	~	miles/vehicles
	~	Average distance to a refueling station
	|

Average Distance Unit Correction=
	1
	~	vehicles*vehicles/stations
	~	Dummy variable ro correct unit errors
	|

Infrastructure in Construction f[Infrastructure]= INTEG (
	Infrastructure Order Rate f[Infrastructure]-Infrastructure Aquisition Rate f[Infrastructure\
		],
		Initial Infrastructure in Construction f[Infrastructure])
	~	stations
	~	Number of stations being built
	|

OEM Revenue i[Technology]=
	Order Fulfillment j[Technology]*MSRP j[Technology]
	~	$/year
	~	Revenue of OEM manufacturers, by platform
	|

Available Infrastructure f[Infrastructure]= INTEG (
	Infrastructure Aquisition Rate f[Infrastructure]-Infrastructure Exits f[Infrastructure\
		]+Exogenous Infrastructure f[Infrastructure
	],
		Initial Infrastructure Availability f[Infrastructure])
	~	stations
	~	Number of refueling stations on the market
	|

Exogenous Infrastructure f[Infrastructure]=
	Stations per Year f[Infrastructure]
	~	stations/year
	~	Additional rate of introduction of new stations
	|

Pump Operating Profits r[Fuels]=
	Pump Fuel Revenues r[Fuels]-Pump Costs r[Fuels]
	~	$/pump/year
	~	Yearly profit of a pump from selling fuel r
	|

Average Familiarity ij[Technology,TechnologyTo]=
	IF THEN ELSE(Platform Introduction Date j[Technology]>Time, 0 , IF THEN ELSE(Technology\
		=TechnologyTo, 1, MIN(1, ZIDZ(Cumulative Familiarity ij[Technology,TechnologyTo],Installed Base i\
		[Technology]))) )
	~	dmnl
	~	Average familiarity of platform i with the platform j. Ranges from 0 (new \
		platform) to 1 (fully familiar).
	|

Marketing Spillover Matrix ij[Technology,TechnologyTo]=
	IF THEN ELSE(Technology=TechnologyTo, 1, IF THEN ELSE(Time<Platform Introduction Date j\
		[TechnologyTo], 0, IF THEN ELSE(
Technology=CONVENTIONAL, 0, IF THEN ELSE( TechnologyTo=CONVENTIONAL, 0, Extent of Marketing Spillovers
	))))
	~	dmnl
	~	Matrix of possible spillovers of marketing efforts from platform i to \
		platform j
	|

VMT SW i[Technology]=
	IF THEN ELSE(Platform Introduction Date j[Technology]<=Time, 1 , 0 )
	~	dmnl
	~	Switch indicating that platform is on the market and is used to drive \
		average number of miles requested by the market
	|

Maximum Fuel Dispensing Capacity per Pump r[Fuels]=
	Fuel Dispensing Rate r[Fuels]*Operating Hours per Year by Fuel r[Fuels]
	~	GGE/year/pump
	~	Maximum possible amount of GGEs a pump is serving per year, by fuel
	|

Introduction Trigger j[TechnologyTo]=
	IF THEN ELSE(Time < Platform Introduction Date j[TechnologyTo], 0, (1-Platform Introduced j\
		[TechnologyTo])/TIME STEP)
	~	dmnl/year
	~	Platform introduced trigger
